; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="EssenceConsoleAddon"
run/main_scene="res://addons/essence_console/Console.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=1250
window/size/viewport_height=690

[editor_plugins]

enabled=PackedStringArray("res://addons/essence_console/plugin.cfg")

[gui]

fonts/dynamic_fonts/use_oversampling=false
theme/default_font_antialiasing=0
theme/default_font_hinting=0
theme/default_font_subpixel_positioning=0

[internationalization]

locale/translations=PackedStringArray("res://addons/essence_console/lang/en_US.po", "res://addons/essence_console/lang/zh_CN.po")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
